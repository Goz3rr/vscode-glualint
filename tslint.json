{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "ordered-imports": false,
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "max-line-length": false,
        "no-console": false
    },
    "rulesDirectory": []
}