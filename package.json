{
    "name": "glua-linter",
    "displayName": "glua-linter",
    "description": "",
    "version": "0.0.1",
    "publisher": "goz3rr",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Linters"
    ],
    "activationEvents": [
        "onLanguage:glua"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.prettyPrint",
                "title": "Pretty print"
            }
        ],
        "configuration": {

        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.1.5",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
