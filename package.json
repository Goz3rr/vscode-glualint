{
    "name": "vscode-glualint",
    "displayName": "vscode-glualint",
    "description": "glualint for vscode",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/goz3rr/vscode-glualint.git"
    },
    "version": "0.1.2",
    "publisher": "goz3rr",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Linters"
    ],
    "activationEvents": [
        "onLanguage:glua"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.1.5",
        "@types/node": "^7.0.43"
    }
}